<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="header.container">
            <block class="Magento\Cms\Block\Block" name="header-top-static-block" before="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">header-top-static-block</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="before.body.end">
            <block class="Magento\Cms\Block\Block" name="footer-bottom-static-block" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer-bottom-static-block</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
